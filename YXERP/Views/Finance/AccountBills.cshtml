@{
    ViewBag.Title = "账户明细";
}
@section css{
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/finance/accountbills"], function (ObjectJS) {
            ObjectJS.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">账户明细</span>
</div>
<div class="search-header">
    <div class="left" title="创建日期">
        <input type="text" id="iptCreateTime" placeholder="创建日期" class="ipt-date"  />
    </div>
    <div class="right searth-module" data-text="关键字搜索..." data-width="160"> </div> 
</div>

<div class="search-body">
    <div class="search-item">
        <div class="name">类型</div>
        <ul class="search-status">
            <li data-id="-1" class="hover">全部</li>
            <li data-id="0" class="">收入</li>
            <li data-id="1" class="">支出</li>
        </ul>
    </div>
</div>
<div class="content-body">
    <table class="table-list mTop10">
        <tr class="tr-header">
            <td class="width100 tLeft">类型</td>
            <td class="width150">发生金额</td>
            <td class="width150">末期余额</td>
            <td class="tLeft">备注</td>
            <td class="width100">操作人</td>
            <td class="width150">操作时间</td>
        </tr>
    </table>
    <div id="pager" class="mTop20"></div>
</div>

