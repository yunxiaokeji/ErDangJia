@{
    ViewBag.Title = "订单跟踪";
}
@section css{
    <link href="/modules/css/stock.css" rel="stylesheet" />
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/storageout/ordertrack"], function (ObjectJS) {
            ObjectJS.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">订单跟踪</span>
</div>
<div class="search-header">
    <div class="left" title="订货时间">
        <input type="text" id="iptCreateTime" placeholder="订货时间" class="ipt-date"  />
    </div>
    <div class="right searth-module" data-text="订单编号/联系人/联系电话/快递单号..." data-width="260"> </div>
</div>
<div class="search-body">
     <div class="search-item">
        <div class="name">出库状态</div>
        <ul class="search-status">
            <li data-id="-1" class="hover">全部</li>
            <li data-id="0">待出库</li>
            <li data-id="1">已出库</li>
        </ul>
    </div>
     <div class="search-item">
        <div class="name">发货状态</div>
        <ul class="search-sendstatus">
            <li data-id="-1" class="hover">全部</li>
            <li data-id="1">未发货</li>
            <li data-id="2">已发货</li>
        </ul>
    </div>
    <div class="search-item">
        <div class="name">退单状态</div>
        <ul class="search-returnstatus">
            <li data-id="-1">全部</li>
            <li data-id="0" class="hover">正常</li>
            <li data-id="1">申请退货</li>
            <li data-id="2">部分退货</li>
            <li data-id="3">已退货</li>
        </ul>
    </div>
</div>
<div class="content-body mTop20">
    <div style="width:100%; overflow:auto;">
        <table class="table-list mTop10">
            <tr class="tr-header">
                <td class="tLeft">订单编号</td>
                <td class="width100">总金额</td>
                <td class="tLeft width150">地址</td>
                <td class="">邮编</td>
                <td class="">联系人</td>
                <td class="width100">联系电话</td>
                <td class="width100">发货状态</td>
                <td class="width150">出库单号</td>
                <td class="">快递公司</td>
                <td class="width150">快递单号</td>
                <td class="width150">订货时间</td>
            </tr>
        </table>
    </div>
    <div id="pager" class="mTop20"></div>
</div>

