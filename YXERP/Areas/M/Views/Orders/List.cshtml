@{
    ViewBag.Title = "订单列表";    
}
@section css{
    <link href="/modules/m/css/style/list.css" rel="stylesheet" />    
}
@section scripts{
    <script type="text/javascript">
        seajs.use("/modules/m/scripts/style/orders.js", function (data) {
            data.init();
        });
    </script>
}
<div class="shade"></div>
<div class="search"> 
    <div class="search-box">
        <span class="iconfont">&#xe605;</span>
        <input type="text" class="txt-search" placeholder="请输入关键字搜索" />
    </div>
    <div class="btn-search">确定</div>

</div>
<div class="topbar">
    <div class="header">
        <div class="header-li li-task-filtertype">
            <div class="btn-task-filtertype">
            <div class="font16">订单状态-全部</div>
            <div class="iconfont">&#xe61c;</div>
            <ul class="task-filtertype">
                <li data-id="-1">全部</li>
                <li data-id="0">待审核</li>
                <li data-id="3">待处理</li>
                <li data-id="1">部分入库</li>
                <li data-id="2">已入库</li>
            </ul>
            </div>
        </div>
        <div class="btnsearch">
            <div class="iconfont iconfont-search">&#xe605;</div>
        </div>
    </div>
</div>
<div class="list" style="margin-top:50px;"></div>
@{Html.RenderPartial("Footer");}