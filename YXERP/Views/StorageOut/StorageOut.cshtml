@{
    ViewBag.Title = "订单出库";
}
@section css{
    <link href="/modules/css/stock.css" rel="stylesheet" />
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/storageout/storageout"], function (ObjectJS) {
            ObjectJS.init();
        });
    </script>
}

<div id="printData" class="hide"></div>

<div id="notprintData" >
<div class="header-box">
    <span class="header-title left">订单出库</span> 
</div>
<div class="search-header">
    <div class="left" title="订货时间">
        <input type="text" id="iptCreateTime" placeholder="订货时间" class="ipt-date"  />
    </div>
    <div class="right searth-module" data-text="订单编号/收货人/联系电话..." data-width="200"></div> 
</div>
<div class="search-body">
     <div class="search-item">
        <div class="name">出库状态</div>
        <ul class="search-status">
            <li data-id="-1">全部</li>
            <li data-id="0" class="hover">未出库</li>
            <li data-id="1">已出库</li>
        </ul>
    </div>
    <div class="search-item">
        <div class="name">发货状态</div>
        <ul class="search-sendstatus">
            <li data-id="-1" class="hover">全部</li>
            <li data-id="1">未发货</li>
            <li data-id="2">已发货</li>
        </ul>
    </div>
</div>
<div class="content-body mTop20">
    <table class="table-list">
        <tr class="tr-header">
            <td class="width30">全选</td>
            <td class="tLeft">订单编号</td>
            <td class="width100">总金额</td>
            <td class="tLeft">地址</td>
            <td class="">收货人</td>
            <td class="width100">联系电话</td>
            <td class="width100">订单状态</td>
            <td class="width150">出库单号</td>
            <td class="">快递公司</td>
            <td class="width150">快递单号</td>
            <td class="width150">订货时间</td>
            <td class="width50">操作</td>
        </tr>
    </table>

    <div class="mTop20">
        <ul class="left"> 
            <li class="hand left width80 mLeft5" id="checkAll">
                <span class="checkbox iconfont mTop5 mRight5"></span>全选
            </li>
            <li class="left">
                <a href="javascript:void(0);" class="btn" id="printOrderOut" >
                     <i class="left iconfont mRight5" style="margin-left: -5px;color:aliceblue">&#xe658;</i>打 印
                </a>
            </li>
        </ul>
        <div id="pager"></div>
        <div class="clear"></div>
    </div> 
</div>
</div>
