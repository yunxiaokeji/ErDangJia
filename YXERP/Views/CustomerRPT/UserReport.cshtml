@{
    ViewBag.Title = "销售客户统计";
}
 <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
@section scripts{
    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/customerrpt/userrpt"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">销售客户统计</span>
</div>
<div class="search-header">
    <ul class="search-tab search-type">
        <li data-type="1" data-id="userTotalRPT" class="hover">销售客户汇总</li>
    </ul>
</div>
<div class="search-header">
    <div class="left" title="客户创建日期">
        <input type="text" id="iptCreateTime" placeholder="客户创建日期" class="ipt-date"  />
    </div>
</div>
<div class="content-body">
    <table id="userTotalRPT" class="table-list-rpt">
        <tr class="tr-header">
            <td class="width50 tLeft"></td>
            <td class="tLeft">名称</td>
            @foreach (var model in (List<CloudSalesEntity.CustomStageEntity>)ViewBag.Stages)
            {
                 <td>@(model.StageName)</td>
            }
            <td>合计</td>
        </tr>
    </table>
</div>

